{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "babel"
    ],
    "rules": {
        "require-jsdoc": 0,
        "linebreak-style": 0,
        "no-invalid-this": 0,
        "babel/no-invalid-this": 1
    },
    "parser": "babel-eslint"

}
